{
  "name": "superkey-docs",
  "version": "1.0.0",
  "author": "@pheralb_",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./src/styles/tailwind.css -o src/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./src/styles/tailwind.css -o src/styles/app.css"
  },
  "dependencies": {
    "@remix-run/node": "1.8.0",
    "@remix-run/react": "1.8.0",
    "@remix-run/vercel": "1.8.0",
    "@vercel/node": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "superkey": "*"
  },
  "devDependencies": {
    "@remix-run/dev": "1.8.0",
    "@remix-run/eslint-config": "1.8.0",
    "@remix-run/serve": "1.8.0",
    "@tailwindcss/typography": "0.5.8",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "postcss": "8.4.19",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=14"
  }
}
